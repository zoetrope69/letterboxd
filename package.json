{
  "name": "letterboxd-api",
  "version": "1.0.1",
  "description": "get public diary data for letterboxd users",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "prepublish": "tsc",
    "prettier": "prettier --write .",
    "lint": "eslint . --config .eslintrc.js",
    "example": "npx ts-node --esm test/example.ts",
    "pretest": "npm run example && npm run prettier && npm run lint",
    "test": "npm run test:js",
    "test:js": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "npm run test:js -- --watch"
  },
  "keywords": [
    "letterboxd"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zeromero-dev/letterboxd-api"
  },
  "author": "Roman <zeromero.dev@gmail.com.com>",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^18.15.11",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^2.6.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "cross-env": "7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.7",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "29.3.1",
    "nock": "13.2.9",
    "prettier": "2.8.1",
    "semantic-release": "19.0.5"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
